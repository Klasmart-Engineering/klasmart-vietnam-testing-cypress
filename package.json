{
  "name": "kidsloophub",
  "version": "1.0.0",
  "description": "Automate different journeys of Kidsloop",
  "main": "index.js",
  "scripts": {
    "test:all": "cypress run --spec \"**/*.features\"",
    "test:each": "cypress run --spec \"**/*.feature\"",
    "test:accessibility": "./node_modules/.bin/cypress-tags run -e TAGS=\"@test\""
  },
  "keywords": [
    "cypress",
    "cucumber",
    "testing"
  ],
  "author": "Swapnali Bhansali",
  "contributors": [
    "Lorena Bettoni"
  ],
  "cypress-cucumber-preprocessor": {
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    },
    "compilerOptions": {
      "types": [
        "cypress",
        "cypress-xpath",
        "cypress-real-events",
        "cypress-file-upload"
      ]
    },
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/integration/"
  },
  "devDependencies": {
    "cypress": "^9.4.1",
    "cypress-cucumber-attach-screenshots-to-failed-steps": "^1.0.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-mailosaur": "^2.5.6",
    "cypress-mailslurp": "^1.3.0",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-xpath": "^1.6.2",
    "jsdom": "^19.0.0",
    "multiple-cucumber-html-reporter": "^1.18.3",
    "prettier": "2.4.1"
  },
  "dependencies": {
    "packagename": "^2.0.1"
  }
}
