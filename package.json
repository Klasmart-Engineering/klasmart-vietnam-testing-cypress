{
  "name": "Admin-Service",
  "version": "1.0.0",
  "description": "Kidsloop Automation Admin-Service",
  "main": "index.js",
  "scripts": {
    "test:all": "cypress run",
    "test:each": "cypress run --spec \"**/*.feature\"",
    "test:accessibility": "./node_modules/.bin/cypress-tags run -e TAGS=\"@smoke\"",
    "cy:run:prod": "cypress open --env ENV=production",
    "cy:run:stg": "cypress open --env ENV=staging",
    "cy:run:qa": "cypress open --env ENV=alpha",
    "cy:run:smoke:alpha": "cypress run -e TAGS=\"@test\" --browser chrome",
    "e2e:reports": "node cypress/cucumber-html-report.js"
  },
  "keywords": [
    "cypress",
    "cucumber",
    "testing"
  ],
  "author": "Lorena Bettoni",
  "contributors": [],
  "repository": {
    "type": "github",
    "url": "https://github.com/KL-Global/testing-cypress"
  },
  "cypress-cucumber-preprocessor": {
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    },
    "compilerOptions": {
      "types": [
        "cypress",
        "cypress-xpath",
        "cypress-real-events",
        "cypress-file-upload"
      ]
    },
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/integration/"
  },
  "devDependencies": {
    "cypress": "^9.5.1",
    "cypress-cucumber-attach-screenshots-to-failed-steps": "1.0.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-xpath": "^1.6.2",
    "jsdom": "^19.0.0",
    "multiple-cucumber-html-reporter": "^1.19.0",
    "prettier": "2.4.1"
  },
  "dependencies": {
    "chai-sorted": "^0.2.0",
    "core-js": "^3.21.1",
    "cucumber": "^7.0.0-rc.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-real-events": "^1.6.0"
  }
}
